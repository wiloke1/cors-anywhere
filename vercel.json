{
  "version": 2,
  "name": "cors-anywhere",
  "builds": [
     { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
     { "src": "/(.*)", "dest": "/index.js" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers" : [
        {
          "key" : "X-Content-Type-Options",
          "value" : "nosniff"
        },
        {
          "key" : "X-Frame-Options",
          "value" : "DENY"
        },
        {
          "key" : "X-XSS-Protection",
          "value" : "1; mode=block"
        }
      ]
    }
  ]
}